{
  "name": "express-ts-backend-generator",
  "version": "2.0.0",
  "type": "commonjs",
  "description": "Clean, maintainable CLI tool to scaffold Express TypeScript projects with DDD architecture",
  "keywords": [
    "cli",
    "express",
    "typescript",
    "scaffold",
    "generator",
    "ddd",
    "clean-architecture",
    "backend",
    "nodejs"
  ],
  "author": "Your Name <your.email@domain.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gelo7212/express-ts-backend-generator.git"
  },
  "bugs": {
    "url": "https://github.com/gelo7212/express-ts-backend-generator/issues"
  },
  "homepage": "https://github.com/gelo7212/express-ts-backend-generator#readme",
  "bin": {
    "express-ts-gen": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
